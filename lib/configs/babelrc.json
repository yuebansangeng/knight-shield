{
  "presets": [ "es2015", "react", "stage-0" ],
  "plugins": [
    "transform-runtime",
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-react-display-name",
    "transform-object-assign",
    "transform-class-properties",
    [
      "transform-es2015-classes", { "loose": true }
    ],
    "transform-proto-to-assign"
  ]
}