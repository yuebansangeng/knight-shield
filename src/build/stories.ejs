
import readme from '../README.md'
import os from 'object-assign'
<% demos.forEach(function(demo, index) { %>
import Component<%=index%> from '../demos/<%=demo.name%>'
<% if (demo.hasEditableProps) { %>
import editableProps<%=index%> from '../demos/<%=demo.name%>/editable-props'
<% } %>
<% if (demo.hasDoc) { %>
import doc<%=index%> from '../demos/<%=demo.name%>/doc.md'
<% } %>
<% }) %>
export default{
  name: "<%=cmpName%>",
  'stories': [
<% demos.forEach(function(demo, index) { %>
    {
      name: '<%=demo.name%>',
      story: os({
        'content': Component<%=index%>,
<% if (demo.hasEditableProps) { %>
        'editableProps': editableProps<%=index%>
<% } else { %>
        'editableProps': []
<% } %>,
<% if (demo.hasDoc) { %>
        'doc': doc<%=index%>
<% }
      })
    },
<% }) %>
  ],
  readme
}
