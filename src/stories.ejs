
export default {
  'name': '<%=name%>',
  'stories': [

<% if(examples.length) { %>
  <% examples.forEach((demo, index) => { %>
    {
      'name': '<%=demo.name%>',
      'story': Object.assign({
        'component': require('<%=cpath%>/examples/<%=demo.name%>'),
      })
    },
  <% }) %>
<% } else { %>
    {
      'name': 'default',
      'story': Object.assign({
        'component': require('<%=cpath%>/src')
      })
    },
<% } %>

  ],
  'readme': <% if(hasReadme){ %>require('<%=cpath%>/README.md')<% }else{ %>''<% } %>
}
